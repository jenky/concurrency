{
    "name": "jenky/concurrency",
    "description": "Common interface for sending concurrent HTTP requests",
    "keywords": [
        "jenky",
        "pool",
        "concurrency",
        "concurrent-requests",
        "parallel-requests",
        "async",
        "await"
    ],
    "homepage": "https://github.com/jenky/concurrency",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynh",
            "email": "contact@lynh.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\Concurrency\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\Concurrency\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": [
            "vendor/bin/phpstan analyse"
        ],
        "cs": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "test": [
            "vendor/bin/phpunit"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage"
        ],
        "check": [
            "@cs --dry-run",
            "@analyse",
            "@test"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
